{
  "AllowedHosts": "*",
  "MongoDbSettings": {
    "Host": "host.docker.internal",
    "Port": "27017"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithClientIp", "WithClientAgent" ],
    "WriteTo": [
      //  {
      //    "Name": "File",
      //    "Args": {
      //      "path": "/logs/youtube_organizer_api_log.txt",
      //      "rollingInterval": "Day"
      //    }
      //  },
      {"Name": "Console"},
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://host.docker.internal:8081"
        }
      }
    ]
  }
}
